        "i18next-http-middleware": "3.2.0",
        "ioredis": "^4.28.0",
        "react": "17.0.2",
        "react-dom": "^17.0.2",
        "requirejs": "^2.3.6",
        "tmp-promise": "3.0.3"
    },
    "devDependencies": {
        "@types/express": "4.17.13",
