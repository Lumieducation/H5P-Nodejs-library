{
    "name": "@lumieducation/h5p-examples",
    "private": "true",
    "version": "10.0.4",
    "description": "",
    "main": "build/express.js",
    "scripts": {
        "build:client": "npx tsc -p ./tsconfig.client.json",
        "build:server": "npx tsc -p ./tsconfig.server.json",
        "build": "npm run build:server && npm run build:client",
        "clean": "rm -rf h5p && rm -rf build && rm -rf coverage && rm -rf node_modules && rm -rf public",
        "start": "node build/express.js",
        "test:e2e:tests": "npx jest --config jest.e2e.config.js --maxWorkers=1",
        "test:e2e": "start-server-and-test start http://localhost:8080 test:e2e:tests",
        "test:server+upload": "start-server-and-test start http://localhost:8080 test:upload",
        "test:upload": "npx ts-node test/e2e/customUpload.ts ${H5P_FILES} ${ERROR_FILE}",
        "test": "echo \"Error: no test specified\" && exit 1",
        "postinstall": "mkdir -p public && cp ../../node_modules/requirejs/require.js ./public/require.js && cp -R ../../node_modules/bootstrap/dist/css/bootstrap.min.css ./public/bootstrap.min.css && cp -R ../../node_modules/@fortawesome/fontawesome-free ./public/ && cp -R ../../node_modules/react ./public/ && cp -R ../../node_modules/react-dom ./public/"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.4.0",
        "@lumieducation/h5p-clamav-scanner": "^10.0.4",
        "@lumieducation/h5p-express": "^10.0.4",
        "@lumieducation/h5p-html-exporter": "^10.0.4",
        "@lumieducation/h5p-mongos3": "^10.0.4",
        "@lumieducation/h5p-redis-lock": "^10.0.4",
        "@lumieducation/h5p-server": "^10.0.4",
        "@lumieducation/h5p-svg-sanitizer": "^10.0.4",
        "body-parser": "1.20.3",
        "bootstrap": "^5.2.3",
        "cache-manager": "4.1.0",
        "cache-manager-redis-store": "2.0.0",
        "debug": "^4.3.4",
        "dotenv": "^16.0.3",
        "express": "4.21.2",
        "express-fileupload": "^1.4.0",
        "fs-extra": "11.3.0",
        "i18next": "24.2.3",
        "i18next-fs-backend": "2.6.0",
        "i18next-http-middleware": "3.7.1",
        "mongodb": "6.15.0",
        "react": "18.3.1",
        "react-dom": "^18.2.0",
        "redis": "^4.7.0",
        "requirejs": "^2.3.6",
        "tmp-promise": "3.0.3"
    },
    "devDependencies": {
        "@types/express": "4.17.21",
        "@types/puppeteer": "7.0.4",
        "@types/react": "18.3.20",
        "puppeteer": "17.1.3",
        "start-server-and-test": "2.0.11"
    }
