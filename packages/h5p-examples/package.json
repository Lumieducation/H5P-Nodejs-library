        "@lumieducation/h5p-server": "^9.1.1",
        "body-parser": "1.20.0",
        "bootstrap": "^5.1.3",
        "cache-manager": "3.6.0",
        "cache-manager-redis-store": "2.0.0",
        "debug": "^4.3.2",
        "dotenv": "^16.0.0",
        "express": "4.17.3",
        "express-fileupload": "^1.2.1",
        "fs-extra": "10.0.1",
