        "@lumieducation/h5p-server": "^9.2.0",
        "body-parser": "1.20.2",
        "bootstrap": "^5.2.3",
        "cache-manager-redis-store": "2.0.0",
        "cache-manager": "4.1.0",
        "debug": "^4.3.4",
        "dotenv": "^16.0.3",
        "express-fileupload": "^1.4.0",
        "express": "4.18.2",
        "fs-extra": "11.1.1",
        "i18next-fs-backend": "2.1.2",
