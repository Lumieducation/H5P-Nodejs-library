        {
            "name": "source",
            "type": "text",
            "label": "copyright-semantics:source",
            "placeholder": "copyright-semantics:source-placeholder",
            "optional": true,
            "regexp": {
                "pattern": "^http[s]?://.+",
                "modifiers": "i"
            }
        },
